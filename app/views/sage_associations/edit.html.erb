<%= title l(:"facturaplus.label_edit_sage_#{@type.underscore}_association") %>

<%= labelled_form_for @sage_association, :html => {:id => 'issue-form', :multipart => true} do |f| %>
  <%= error_messages_for 'sage_associations' %>
  <div class="box tabular">
    <div id="all_attributes">
        <%= render :partial => 'sage_associations/form', :locals => {:f => f} %>
    </div>

  </div>

  <%= submit_tag l(:button_update) %>
<% end %>

<%= link_to l(:button_back), sage_associations_path %>
